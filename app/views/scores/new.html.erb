<% content_for :title, 'Score' %>

<div id="Score">
<div class="row">
<div class="medium-8 medium-centered columns">

  <%= form_for(@score) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
    <%= f.text_field :name, class: 'radius', placeholder: 'score-name' %>
    <%= f.hidden_field :data %>
    <%= f.submit '新規作曲', class: 'button radius expand' %>
  <% end %>

</div>
</div>
</div>